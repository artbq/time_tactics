<h1>Login</h1>

<%= form_for @login do |f| %>
  <%= render partial: "shared/errors", locals: {object: @login} %>

  <%= f.label :username %><br/>
  <%= f.text_field :username %><br/><br/>

  <%= f.label :password %><br/>
  <%= f.password_field :password %><br/><br/>

  <%= f.submit t("views.logins.new.submit") %>
<% end %>
